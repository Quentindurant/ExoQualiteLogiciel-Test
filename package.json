{
  "name": "demo-qualite-calculatrice",
  "version": "1.0.0",
  "description": "Projet legacy pour formation qualit√© logicielle - Fonction monstrueuse",
  "main": "dist/calculator/calculator.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "start": "npm run build && node dist/calculator/calculator.js",
    "start:web": "npm run build && npx http-server . -p 8081 -o /public/index-legacy.html",
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && node dist/calculator/calculator.js\"",
    "dev:web": "nodemon --watch src --ext ts,html --exec \"npm run build && npx http-server . -p 8081\"",
    "examples": "npm run build && node dist/calculator/examples.js",
    "clean": "rm -rf dist",
    "lint": "eslint src --fix",
    "format": "prettier --write src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "check": "npm run lint && npm run format"
  },
  "keywords": [
    "legacy",
    "refactoring",
    "clean-code",
    "formation",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "http-server": "^14.1.1",
    "nodemon": "3.1.11",
    "prettier": "^3.7.4",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  }
}